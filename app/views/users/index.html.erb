<%= render 'template2', book: @book, newuser: @newuser %>
<div class="col-md-8 offset-md-1">
  <h2>Books</h2>
  <table class="table">
    <thead>
      <tr>
       <th>image</th>
       <th>name</th>
       <th colspan="3"></th>
      </tr>
    </thead>

    <% @newuser.each do |newuser| %>
      <tbody>
        <tr>
          <td>
            <%= image_tag newuser.get_image(80, 80) %>
          </td>

          <td><%= newuser.name %></td>

          <td>
            <%= link_to user_path(newuser.id) do %>
              Show
            <%= end %>
          </td>
        </tr>
      </tbody>
  </table>
</div>